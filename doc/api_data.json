[  {    "type": "get",    "url": "/apply",    "title": "Runs the rule engine with the loaded set of rules on a given message and tag.",    "version": "0.0.1",    "name": "Apply_Rules__",    "group": "YKW",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>json</p> ",            "optional": false,            "field": "message",            "description": "<p>The message on which you want to run the rule engine.</p> "          },          {            "group": "Parameter",            "type": "<p>string</p> ",            "optional": false,            "field": "tag",            "description": "<p>The specific set of rules that you want to run the rule engine on.</p> "          }        ]      },      "examples": [        {          "title": "Request Sample",          "content": "{\n   \"message\": {\n       \"integer\": 1\n   },\n   \"tag\": {\n       \"natural\"\n   }\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "200": [          {            "group": "200",            "type": "<p>json</p> ",            "optional": false,            "field": "message",            "description": "<p>The message after the rule engine has been run.</p> "          },          {            "group": "200",            "type": "<p>json</p> ",            "optional": false,            "field": "_meta",            "description": "<p>The statistics (how many actions / conditions were run / time took) on the rule engine for this message.</p> "          }        ]      },      "examples": [        {          "title": "YKW-Response:",          "content": "    {\n   \"message\": {\n       integer: 1,\n       _meta: {\n           id: 1459408164318\n       },\n       is_natural: 1\n   },\n   \"_meta\": {\n       \"ts\": {\n           \"rules_loaded\": 1459408164036,\n           \"start\": 1459408164069,\n           \"end\": 1459408164071\n       },\n       \"rules\": {\n           \"1\": {\n               \"conditions\": {\n                   \"1\": true\n               },\n               \"applied\": true,\n               \"actions\": {},\n               \"total_actions\": 1,\n               \"total_conditions\": 1\n           }\n       },\n       \"ruleEngineHash\": \"9edb27c675b4a567ce5e13ae976cc12347c72e8b\"\n   }\n}",          "type": "json"        }      ]    },    "filename": "webServer/server.js",    "groupTitle": "YKW"  },  {    "type": "post",    "url": "/load",    "title": "Loads a rule set to the YKW instace.",    "version": "0.0.1",    "name": "Load_Rules__",    "group": "YKW",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>Object[]</p> ",            "optional": false,            "field": "rules",            "description": "<p>Array of rule objects. This will contain the rule , its conditions and actions .</p> "          }        ]      },      "examples": [        {          "title": "Request Sample",          "content": "[\n {\n  rule: {\n      id: 1,\n      name: 'Natural Number ',\n      external_reference: '',\n      conditionsOperator: '&&',\n      priority: 170001\n  },\n  'rule_tags': {\n      tags: 'natural'\n  },\n  rule_condition: {\n      id: 1,\n      key: 'integer',\n      operation: '>',\n      value: '0'\n  },\n  rule_action: {\n      id: 1,\n      action: 'SET_VARIABLE',\n      key: 'is_natural',\n      value: 1\n  }\n }\n ]",          "type": "json"        }      ]    },    "success": {      "fields": {        "204": [          {            "group": "204",            "type": "<p>String</p> ",            "optional": false,            "field": "OK",            "description": "<p>The rules have been loaded into the rule engine instance.</p> "          }        ]      }    },    "error": {      "fields": {        "400": [          {            "group": "400",            "type": "<p>String</p> ",            "optional": false,            "field": "NO-RULES-PASSED",            "description": "<p>This says that the rules have not been passed or passed in an incorrect format.</p> "          }        ]      }    },    "filename": "webServer/server.js",    "groupTitle": "YKW"  }]